{
    "ForceRewrite": false,
    "MaxProcessThreads": 4,
    "MaxPreProcessThreads": 12,
    "LogLevel": "debug",
    "Input": {
        "Storage": {
            "Type": "local-unix",
            "Config": {
                "MaxDepth": 3,
                "Path": "/tmp/thumbnailing/full/"
            }
        },
        "KnownExtensions": [
            "jpg",
            "jpeg",
            "png"
        ]
    },
    "Converter": {
        "Type": "webp",
        "Config": {
            "Quality": 80,
            "Size": {
                "MaxWidth": 800,
                "MaxHeight": 0
            }
        },
        "Output": {
            "Storage": {
                "Type": "local-unix",
                "Config": {
                    "Path": "/tmp/thumbnailing/thumbnails/",
                    "DirPermissionMode": "0755",
                    "FilePermissionMode": "0644",
                    "AttributesImplementation": "xattr"
                }
            }
        }
    }
}